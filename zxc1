meme_dict = {
            "КРИНЖ": "Что-то очень странное или стыдное",
            "ЛОЛ": "Что-то очень смешное",
            'ТИЛЬТ':'состояние сильного раздражения',
            'ВАЙБ':'атмосфера',
            'КРАШ':'объект влюблённости',
            'РОФЛ':'шутка',
            'ЩИЩ':'одобрение или восторг',
            'АГРИТЬСЯ':'злиться',
            'КРИП':'любое существо, не являющееся героем, постройкой или курьером'
            }
zxc = 'да'
while zxc == 'да':
    word = input('Введите непонятное слово (большими буквами!): ')
    if word in meme_dict.keys():
        print(meme_dict[word])
    else:
        print('такого слова нет в словаре')
    zxc = input('хотите ли вы узнать ещё о каком-нибудь слове?(да или нет)')    
if zxc == 'нет':
    print('удачи!')
